{
    "name": "comalaworkflow",
    "displayName": "Comala Workflow Syntax Highlighting",
    "description": "Syntax highlighting for Comala Workflows. Not sure what Comala is? Check out http://www.comalatech.com/products/comalaWorkflows for more info!",
    "version": "0.1.0.0",
    "publisher": "dschneider",
    "engines": {
        "vscode": "^0.10.10"
    },
    "icon": "images/comalaworkflow.png",
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "comalaworkflow",
                "aliases": [
                    "ComalaWorkflow"
                ],
                "extensions": [
                    ".comala",
                    ".workflow",
                    ".comalaworkflow",
                    ".cwf"
                ],
                "configuration": "./comalaworkflow.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "comalaworkflow",
                "scopeName": "text.comalaworkflow",
                "path": "./syntaxes/comalaworkflow.tmLanguage"
            }
        ],
        "snippets": [
            {
                "language": "comalaworkflow",
                "path": "./snippets/comalaworkflow.json"
            }
        ]
    }
}
